   },
   "homepage": "https://github.com/Fuji-Noobie-Ghost/e-fanakalo#readme",
   "devDependencies": {
      "@types/compression": "^1.7.2",
      "@types/express": "^4.17.13",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "nodemon": "^2.0.19",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.0.2",
